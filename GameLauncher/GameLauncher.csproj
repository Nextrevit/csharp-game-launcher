<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>images\Icon.ico</ApplicationIcon>
    <AssemblyName>Renvirons Project Launcher</AssemblyName>
    <RootNamespace>GameLauncher</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="fonts\LexendDeca-Regular.ttf" />
    <None Remove="images\Icon.ico" />
    <None Remove="images\LauncherBackground.png" />
    <None Remove="images\Renvirons Text.png" />
    <None Remove="images\RenvironsLogo.png" />
    <None Remove="LauncherBackground.png" />
    <None Remove="LauncherBackgroundNEW.png" />
    <None Remove="RenvironsLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="fonts\LexendDeca-Regular.ttf" />
    <Resource Include="images\Icon.ico" />
    <Resource Include="images\LauncherBackground.png" />
    <Resource Include="images\Renvirons Text.png" />
    <Resource Include="images\RenvironsLogo.png" />
    <Resource Include="LauncherBackground.png" />
    <Resource Include="LauncherBackgroundNEW.png" />
    <Resource Include="RenvironsLogo.png" />
  </ItemGroup>

</Project>